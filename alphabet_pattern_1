public class Main
{
	public static void main(String[] args) {
		char c='a';
		int n=5;
		for(int i=1; i<=n; i++) {
			for(int j=1; j<=i; j++) {
				if(j%2!=0){
				    System.out.print(Character.toUpperCase(c++) +" ");
				}else{
				    System.out.print(c++ +" ");
				}
			}
			System.out.println();
		}
	}
}
